{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "sourceMap": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "dumi-test": ["packages/comTest/src"],
      "dumi-test/*": ["packages/comTest/src/*", "*"]
    },
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./typings"]
  },
  "include": [".dumirc.ts", "packages/**/*"]
}
